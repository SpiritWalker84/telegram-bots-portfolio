# Токен бота от @BotFather
BOT_TOKEN=your_bot_token_here

# ID администратора (можно получить у @userinfobot)
ADMIN_ID=your_admin_id_here

# Путь к базе данных (опционально, по умолчанию booking_bot.db)
DB_PATH=booking_bot.db

# Уровень логирования (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Файл для логов (опционально)
LOG_FILE=bot.log

# Часы работы (начало и конец)
WORKING_HOURS_START=9
WORKING_HOURS_END=18

# Интервал между записями в минутах
APPOINTMENT_INTERVAL=30




